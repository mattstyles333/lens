AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA","file":"BasicElementsSection.vue","sourceRoot":"src/views/components","sourcesContent":["<template>\n  <div class=\"wrapper\">\n    <div id=\"buttons\">\n      <div class=\"title\">\n        <h3>\n          Buttons <br>\n          <small>Pick your style</small>\n        </h3>\n      </div>\n      <div class=\"md-layout\">\n        <div class=\"md-layout-item md-size-66 mx-auto\">\n          <md-button class=\"md-success\">\n            Default\n          </md-button>\n          <md-button class=\"md-success md-round\">\n            Round\n          </md-button>\n          <md-button\n            class=\"md-success\"\n          >\n            <md-icon>favorite</md-icon> With Icon\n          </md-button>\n          <md-button\n            class=\"md-success md-just-icon md-round\"\n          >\n            <md-icon>favorite</md-icon>\n          </md-button>\n          <md-button class=\"md-success md-simple\">\n            Simple\n          </md-button>\n        </div>\n      </div>\n      <div class=\"title\">\n        <h3><small>Pick your size</small></h3>\n      </div>\n      <div class=\"md-layout\">\n        <div class=\"md-layout-item md-size-66 mx-auto md-xsmall-size-100\">\n          <div class=\"vertical-center\">\n            <md-button class=\"md-success md-sm\">\n              Small\n            </md-button>\n            <md-button class=\"md-success\">\n              Regular\n            </md-button>\n            <md-button class=\"md-success md-lg\">\n              Large\n            </md-button>\n          </div>\n        </div>\n      </div>\n      <div class=\"title\">\n        <h3><small>Pick your color</small></h3>\n      </div>\n      <div class=\"md-layout\">\n        <div class=\"md-layout-item md-size-66 mx-auto\">\n          <md-button>Default</md-button>\n          <md-button class=\"md-primary\">\n            Primary\n          </md-button>\n          <md-button class=\"md-info\">\n            Info\n          </md-button>\n          <md-button class=\"md-success\">\n            Success\n          </md-button>\n          <md-button class=\"md-warning\">\n            Warning\n          </md-button>\n          <md-button class=\"md-danger\">\n            Danger\n          </md-button>\n          <md-button class=\"md-rose\">\n            Rose\n          </md-button>\n        </div>\n      </div>\n    </div>\n    <!-- end buttons -->\n    <div class=\"space-50\" />\n\n    <!-- Social Buttons -->\n    <div\n      id=\"social-buttons\"\n      class=\"social-buttons-demo\"\n    >\n      <div class=\"title\">\n        <h3>Social Buttons</h3>\n      </div>\n      <div\n        v-for=\"color in colors\"\n        :key=\"color.name\"\n        class=\"md-layout\"\n      >\n        <div\n          class=\"md-layout-item md-size-25 md-small-size-50 md-xsmall-size-100\"\n        >\n          <h3 v-if=\"color.name === 'twitter'\">\n            <small>Default</small>\n          </h3>\n          <md-button\n            :class=\"'md-' + color.name\"\n          >\n            <i :class=\"'fab fa-' + color.icon\" />\n            {{ color.btnText }}\n          </md-button>\n        </div>\n        <div\n          class=\"md-layout-item md-size-10 md-small-size-10 md-xsmall-size-100\"\n        >\n          <h3 v-if=\"color.name === 'twitter' && socialBtn\">\n            <small>&nbsp;</small>\n          </h3>\n          <md-button\n            :class=\"'md-just-icon md-' + color.name\"\n          >\n            <i :class=\"'fab fa-' + color.icon\" />\n          </md-button>\n        </div>\n        <div\n          class=\"md-layout-item md-size-10 md-small-size-40 md-xsmall-size-100\"\n        >\n          <h3 v-if=\"color.name === 'twitter' && socialBtn\">\n            <small>&nbsp;</small>\n          </h3>\n          <md-button\n            :class=\"'md-just-icon md-round md-' + color.name\"\n          >\n            <i :class=\"'fab fa-' + color.icon\" />\n          </md-button>\n        </div>\n        <div\n          class=\"md-layout-item md-size-10 md-small-size-10 md-xsmall-size-100\"\n        >\n          <h3 v-if=\"color.name === 'twitter'\">\n            <small>Simple</small>\n          </h3>\n          <md-button\n            :class=\"'md-just-icon md-simple md-' + color.name\"\n          >\n            <i :class=\"'fab fa-' + color.iconSquare\" />\n          </md-button>\n        </div>\n        <div\n          class=\"md-layout-item md-size-25 md-small-size-25 md-xsmall-size-100\"\n        >\n          <h3 v-if=\"color.name === 'twitter' && socialBtn\">\n            <small>&nbsp;</small>\n          </h3>\n          <md-button\n            :class=\"'md-simple md-' + color.name\"\n          >\n            <i :class=\"'fab fa-' + color.iconSquare\" />\n            {{ color.btnText }}\n          </md-button>\n        </div>\n      </div>\n    </div>\n    <!-- end Social Buttons -->\n\n    <!-- Inputs -->\n    <div id=\"inputs\">\n      <div class=\"title\">\n        <h3>Inputs</h3>\n      </div>\n      <div class=\"md-layout\">\n        <div\n          class=\"md-layout-item md-size-25 md-xsmall-size-100 md-small-size-50 md-medium-size-25\"\n        >\n          <md-field>\n            <md-input\n              v-model=\"initial\"\n              placeholder=\"Regular\"\n            />\n          </md-field>\n        </div>\n        <div\n          class=\"md-layout-item md-size-25 md-xsmall-size-100 md-small-size-50 md-medium-size-25\"\n        >\n          <md-field>\n            <label>With Floating Label</label>\n            <md-input\n              v-model=\"floatingLabel\"\n              type=\"text\"\n            />\n          </md-field>\n        </div>\n        <div\n          class=\"md-layout-item md-size-25 md-xsmall-size-100 md-small-size-50 md-medium-size-25\"\n        >\n          <md-field class=\"md-valid\">\n            <label>Success Input</label>\n            <md-input\n              v-model=\"success\"\n              type=\"text\"\n            />\n            <md-icon>done</md-icon>\n          </md-field>\n        </div>\n        <div\n          class=\"md-layout-item md-size-25 md-xsmall-size-100 md-small-size-50 md-medium-size-25\"\n        >\n          <md-field class=\"md-error\">\n            <label>Error Input</label>\n            <md-input\n              v-model=\"error\"\n              type=\"text\"\n            />\n            <md-icon>clear</md-icon>\n          </md-field>\n        </div>\n        <div\n          class=\"md-layout-item md-size-25 md-xsmall-size-100 md-small-size-50 md-medium-size-25\"\n        >\n          <md-field class=\"md-form-group\">\n            <md-icon>face</md-icon>\n            <md-input\n              v-model=\"withMIcon\"\n              placeholder=\"With Material Icons\"\n            />\n          </md-field>\n        </div>\n        <div\n          class=\"md-layout-item md-size-25 md-xsmall-size-100 md-small-size-50 md-medium-size-25\"\n        >\n          <md-field class=\"md-form-group\">\n            <i class=\"fas fa-users\" />\n            <md-input\n              v-model=\"withFaIcon\"\n              placeholder=\"With Font Awesome Icons\"\n            />\n          </md-field>\n        </div>\n      </div>\n    </div>\n    <!-- end inputs -->\n\n    <div class=\"space-50\" />\n\n    <!-- Checkboxes/Radios/Toggle -->\n    <div id=\"checkRadios\">\n      <div class=\"md-layout\">\n        <div\n          class=\"md-layout-item md-size-25 md-xsmall-size-100 md-small-size-50 md-medium-size-25\"\n        >\n          <div class=\"title\">\n            <h3>Checkboxes</h3>\n          </div>\n          <div class=\"flex-column\">\n            <md-checkbox v-model=\"checkbox1\">\n              Checked\n            </md-checkbox>\n            <md-checkbox v-model=\"checkbox2\">\n              Unchecked\n            </md-checkbox>\n            <md-checkbox\n              v-model=\"checkbox3\"\n              disabled\n            >\n              Disabled Checked\n            </md-checkbox>\n            <md-checkbox\n              v-model=\"checkbox4\"\n              disabled\n            >\n              Disabled Unchecked\n            </md-checkbox>\n          </div>\n        </div>\n        <div\n          class=\"md-layout-item md-size-25 md-xsmall-size-100 md-small-size-50 md-medium-size-25\"\n        >\n          <div class=\"title\">\n            <h3>Radios</h3>\n          </div>\n          <div class=\"flex-column\">\n            <md-radio\n              v-model=\"radio1\"\n              :value=\"true\"\n            >\n              Radio is on\n            </md-radio>\n            <md-radio\n              v-model=\"radio1\"\n              :value=\"false\"\n            >\n              Radio is off\n            </md-radio>\n            <md-radio\n              v-model=\"radio2\"\n              :value=\"true\"\n              disabled\n            >\n              Disabled Radio is on\n            </md-radio>\n            <md-radio\n              v-model=\"radio2\"\n              :value=\"false\"\n              disabled\n            >\n              Disabled Radio is off\n            </md-radio>\n          </div>\n        </div>\n        <div\n          class=\"md-layout-item md-size-25 md-xsmall-size-100 md-small-size-50 md-medium-size-25\"\n        >\n          <div class=\"title\">\n            <h3>Toggle Buttons</h3>\n          </div>\n          <div class=\"flex-column\">\n            <md-switch v-model=\"switch1\">\n              Toogle is on\n            </md-switch>\n            <md-switch v-model=\"switch2\">\n              Toogle is off\n            </md-switch>\n          </div>\n        </div>\n      </div>\n    </div>\n    <!-- end Checkboxes/Radios/Toggle -->\n\n    <div class=\"space-50\" />\n\n    <!-- Customizable Select & Dropdowns -->\n    <div id=\"customizableSelect\">\n      <div class=\"md-layout\">\n        <div class=\"md-layout-item md-size-50 md-xsmall-size-100\">\n          <div class=\"title\">\n            <h3>Customizable Select</h3>\n          </div>\n          <div class=\"md-layout\">\n            <div class=\"md-layout-item\">\n              <md-field>\n                <label for=\"movie\">Movie</label>\n                <md-select\n                  id=\"movie\"\n                  v-model=\"movie\"\n                  name=\"movie\"\n                >\n                  <md-option value=\"fight-club\">\n                    Fight Club\n                  </md-option>\n                  <md-option value=\"godfather\">\n                    Godfather\n                  </md-option>\n                  <md-option value=\"godfather-ii\">\n                    Godfather II\n                  </md-option>\n                  <md-option value=\"godfather-iii\">\n                    Godfather III\n                  </md-option>\n                  <md-option value=\"godfellas\">\n                    Godfellas\n                  </md-option>\n                  <md-option value=\"pulp-fiction\">\n                    Pulp Fiction\n                  </md-option>\n                  <md-option value=\"scarface\">\n                    Scarface\n                  </md-option>\n                </md-select>\n              </md-field>\n            </div>\n            <div class=\"md-layout-item\">\n              <md-field>\n                <label for=\"movies\">Movies</label>\n                <md-select\n                  id=\"movies\"\n                  v-model=\"selectedMovies\"\n                  name=\"movies\"\n                  multiple\n                >\n                  <md-option value=\"fight-club\">\n                    Fight Club\n                  </md-option>\n                  <md-option value=\"godfather\">\n                    Godfather\n                  </md-option>\n                  <md-option value=\"godfather-ii\">\n                    Godfather II\n                  </md-option>\n                  <md-option value=\"godfather-iii\">\n                    Godfather III\n                  </md-option>\n                  <md-option value=\"godfellas\">\n                    Godfellas\n                  </md-option>\n                  <md-option value=\"pulp-fiction\">\n                    Pulp Fiction\n                  </md-option>\n                  <md-option value=\"scarface\">\n                    Scarface\n                  </md-option>\n                </md-select>\n              </md-field>\n            </div>\n          </div>\n        </div>\n        <div class=\"md-layout-item md-size-50 md-small-size-100\">\n          <div class=\"title\">\n            <h3>Dropdown & Dropup</h3>\n          </div>\n          <div class=\"md-layout\">\n            <div class=\"md-layout-item md-size-50 md-xsmall-size-100\">\n              <drop-down\n                direction=\"down\"\n                multi-level\n              >\n                <md-button\n                  slot=\"title\"\n                  class=\"md-button md-success md-round md-block dropdown-toggle\"\n                  data-toggle=\"dropdown\"\n                >\n                  Multilevel Dropdown\n                </md-button>\n                <ul\n                  class=\"dropdown-menu\"\n                  :class=\"{ 'dropdown-menu-right': responsive }\"\n                >\n                  <li><a href=\"#\">Action</a></li>\n                  <li><a href=\"#\">Another Action</a></li>\n                  <li>\n                    <a\n                      class=\"dropdown-toggle\"\n                      :class=\"{ open: multiLevel }\"\n                      @click=\"toggleMultiLevel\"\n                    >Submenu</a>\n                    <ul class=\"dropdown-menu\">\n                      <li><a href=\"#\">Submenu action</a></li>\n                      <li><a href=\"#\">Submenu action</a></li>\n                      <li>\n                        <a\n                          class=\"dropdown-toggle\"\n                          :class=\"{ open: multiLevel2 }\"\n                          @click=\"toggleMultiLevel2()\"\n                        >Subsubmenu</a>\n                        <ul\n                          class=\"dropdown-menu\"\n                          :class=\"{ 'dropdown-menu-right': responsive }\"\n                        >\n                          <li><a href=\"#\">Subsubmenu action 1</a></li>\n                          <li><a href=\"#\">Subsubmenu action 2</a></li>\n                        </ul>\n                      </li>\n                      <li>\n                        <a\n                          class=\"dropdown-toggle\"\n                          :class=\"{ open: multiLevel3 }\"\n                          @click=\"toggleMultiLevel3\"\n                        >Second Subsubmenu</a>\n                        <ul\n                          class=\"dropdown-menu\"\n                          :class=\"{ 'dropdown-menu-right': responsive }\"\n                        >\n                          <li><a href=\"#\">Subsubmenu action 1</a></li>\n                          <li><a href=\"#\">Subsubmenu action 2</a></li>\n                        </ul>\n                      </li>\n                    </ul>\n                  </li>\n                </ul>\n              </drop-down>\n            </div>\n            <div class=\"md-layout-item md-size-50 md-xsmall-size-100\">\n              <drop-down direction=\"up\">\n                <md-button\n                  slot=\"title\"\n                  class=\"md-button md-success md-round md-block dropdown-toggle\"\n                  data-toggle=\"dropdown\"\n                >\n                  Dropup\n                </md-button>\n                <ul class=\"dropdown-menu dropdown-menu-right\">\n                  <h6 class=\"dropdown-header\">\n                    Dropdown header\n                  </h6>\n                  <li><a href=\"#\">Action</a></li>\n                  <li><a href=\"#\">Another Action</a></li>\n                  <li><a href=\"#\">Something else here</a></li>\n                  <li><div class=\"dropdown-divider\" /></li>\n                  <li><a href=\"#\">Separated Link</a></li>\n                  <li><div class=\"dropdown-divider\" /></li>\n                  <li><a href=\"#\">One more separated link</a></li>\n                </ul>\n              </drop-down>\n            </div>\n            <div class=\"md-layout-item md-size-50 mx-auto md-xsmall-size-100\">\n              <drop-down direction=\"down\">\n                <md-button\n                  slot=\"title\"\n                  class=\"md-button md-success md-round md-block dropdown-toggle\"\n                  data-toggle=\"dropdown\"\n                >\n                  Dropdown\n                </md-button>\n                <ul class=\"dropdown-menu dropdown-menu-right\">\n                  <h6 class=\"dropdown-header\">\n                    Dropdown header\n                  </h6>\n                  <li><a href=\"#\">Action</a></li>\n                  <li><a href=\"#\">Another Action</a></li>\n                  <li><a href=\"#\">Something else here</a></li>\n                  <li><div class=\"dropdown-divider\" /></li>\n                  <li><a href=\"#\">Separated Link</a></li>\n                  <li><div class=\"dropdown-divider\" /></li>\n                  <li><a href=\"#\">One more separated link</a></li>\n                </ul>\n              </drop-down>\n            </div>\n          </div>\n        </div>\n      </div>\n    </div>\n    <!-- end Customizable Select & Dropdowns -->\n\n    <div class=\"space-70\" />\n\n    <div id=\"textarea\">\n      <div class=\"md-layout\">\n        <div class=\"md-layout-item md-size-50\">\n          <div class=\"title\">\n            <h3>Textarea</h3>\n          </div>\n          <md-field maxlength=\"5\">\n            <label>You can write your text here..</label>\n            <md-textarea v-model=\"aboutme\" />\n          </md-field>\n        </div>\n        <div class=\"md-layout-item md-size-50\">\n          <div class=\"title\">\n            <h3>Tags</h3>\n          </div>\n          <md-chips\n            v-model=\"fruits\"\n            class=\"md-primary\"\n            md-placeholder=\"Add city...\"\n          />\n        </div>\n      </div>\n    </div>\n\n    <!-- Progress/Pagination -->\n    <div id=\"progress-pagination\">\n      <div class=\"md-layout\">\n        <div class=\"md-layout-item md-size-50 md-xsmall-size-100\">\n          <div class=\"title\">\n            <h3>Progress</h3>\n          </div>\n          <md-progress-bar\n            class=\"md-primary\"\n            :md-value=\"amount\"\n          />\n          <md-progress-bar\n            class=\"md-info\"\n            :md-value=\"amount2\"\n          />\n          <md-progress-bar\n            class=\"md-warning\"\n            md-mode=\"buffer\"\n            :md-value=\"buffer\"\n            :md-buffer=\"buffer\"\n          />\n        </div>\n        <div class=\"md-layout-item md-size-50 md-xsmall-size-100\">\n          <div class=\"title\">\n            <h3>Pagination</h3>\n          </div>\n          <pagination\n            v-model=\"defaultPagination\"\n            no-arrows\n            :page-count=\"5\"\n          />\n          <pagination\n            v-model=\"infoPagination\"\n            class=\"pagination-info\"\n            with-text\n            :page-count=\"5\"\n          />\n        </div>\n      </div>\n    </div>\n    <!-- end progress/pagination -->\n\n    <div class=\"space-50\" />\n\n    <!-- Sliders -->\n    <div id=\"sliders\">\n      <div class=\"md-layout\">\n        <div class=\"md-layout-item md-size-50 md-xsmall-size-100\">\n          <div class=\"title\">\n            <h3>Sliders</h3>\n          </div>\n          <slider v-model=\"sliders.simple\" />\n\n          <slider\n            v-model=\"sliders.rangeSlider\"\n            type=\"info\"\n            :connect=\"true\"\n          />\n        </div>\n        <div class=\"md-layout-item md-size-50 md-xsmall-size-100\">\n          <div class=\"title\">\n            <h3>Badges</h3>\n          </div>\n          <badge\n            type=\"default\"\n            class=\"badge-pill\"\n          >\n            Default\n          </badge>\n          <badge\n            type=\"primary\"\n            class=\"badge-pill\"\n          >\n            Primary\n          </badge>\n          <badge\n            type=\"info\"\n            class=\"badge-pill\"\n          >\n            Info\n          </badge>\n          <badge\n            type=\"success\"\n            class=\"badge-pill\"\n          >\n            Success\n          </badge>\n          <badge\n            type=\"warning\"\n            class=\"badge-pill\"\n          >\n            Warning\n          </badge>\n          <badge\n            type=\"danger\"\n            class=\"badge-pill\"\n          >\n            Danger\n          </badge>\n          <badge\n            type=\"rose\"\n            class=\"badge-pill\"\n          >\n            Rose\n          </badge>\n        </div>\n      </div>\n    </div>\n    <!-- end sliders -->\n  </div>\n</template>\n\n<script>\nimport { Pagination } from \"@/components\";\nimport { Slider } from \"@/components\";\nimport { Badge } from \"@/components\";\n\nexport default {\n  components: {\n    Pagination,\n    Slider,\n    Badge\n  },\n  data() {\n    return {\n      aboutme: null,\n      responsive: false,\n      socialBtn: false,\n      defaultPagination: 3,\n      infoPagination: 3,\n      initial: null,\n      floatingLabel: null,\n      success: null,\n      error: null,\n      multiLevel: false,\n      multiLevel2: false,\n      multiLevel3: false,\n      withMIcon: null,\n      withFaIcon: null,\n      checkbox1: true,\n      checkbox2: null,\n      checkbox3: true,\n      checkbox4: null,\n      radio1: true,\n      radio2: false,\n      switch1: true,\n      switch2: null,\n      amount: 30,\n      amount2: 60,\n      buffer: 40,\n      movie: \"godfather\",\n      selectedMovies: [],\n      sliders: {\n        simple: 40,\n        rangeSlider: [20, 60]\n      },\n      fruits: [\"Amsterdam\", \"Washington\", \"Sydney\", \"Beijing\"],\n      colors: [\n        {\n          name: \"twitter\",\n          icon: \"twitter\",\n          iconSquare: \"twitter\",\n          btnText: \"connect with twitter\"\n        },\n        {\n          name: \"facebook\",\n          icon: \"facebook-f\",\n          iconSquare: \"facebook-square\",\n          btnText: \"share · 2.2k\"\n        },\n        {\n          name: \"google\",\n          icon: \"google-plus-g\",\n          iconSquare: \"google-plus-square\",\n          btnText: \"share on google+\"\n        },\n        {\n          name: \"linkedin\",\n          icon: \"linkedin\",\n          iconSquare: \"linkedin\",\n          btnText: \"connect with linkedin\"\n        },\n        {\n          name: \"pinterest\",\n          icon: \"pinterest-p\",\n          iconSquare: \"pinterest\",\n          btnText: \"pin it · 212\"\n        },\n        {\n          name: \"youtube\",\n          icon: \"youtube\",\n          iconSquare: \"youtube\",\n          btnText: \"view on youtube\"\n        },\n        {\n          name: \"repost\",\n          icon: \"tumblr\",\n          iconSquare: \"tumblr-square\",\n          btnText: \"repost\
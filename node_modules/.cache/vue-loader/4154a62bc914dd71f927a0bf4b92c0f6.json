{"remainingRequest":"/home/envisible/Documents/code/vue-material-kit-pro-v1.2.2/vue-material-kit-v1.2.2/node_modules/vue-loader/lib/index.js??vue-loader-options!/home/envisible/Documents/code/vue-material-kit-pro-v1.2.2/vue-material-kit-v1.2.2/src/views/examples/Shopping.vue?vue&type=script&lang=js&","dependencies":[{"path":"/home/envisible/Documents/code/vue-material-kit-pro-v1.2.2/vue-material-kit-v1.2.2/src/views/examples/Shopping.vue","mtime":1576662228000},{"path":"/home/envisible/Documents/code/vue-material-kit-pro-v1.2.2/vue-material-kit-v1.2.2/node_modules/cache-loader/dist/cjs.js","mtime":499162500000},{"path":"/home/envisible/Documents/code/vue-material-kit-pro-v1.2.2/vue-material-kit-v1.2.2/node_modules/babel-loader/lib/index.js","mtime":315532800000},{"path":"/home/envisible/Documents/code/vue-material-kit-pro-v1.2.2/vue-material-kit-v1.2.2/node_modules/cache-loader/dist/cjs.js","mtime":499162500000},{"path":"/home/envisible/Documents/code/vue-material-kit-pro-v1.2.2/vue-material-kit-v1.2.2/node_modules/vue-loader/lib/index.js","mtime":499162500000}],"contextDependencies":[],"result":[{"type":"Buffer","data":"base64:"},{"version":3,"sources":["Shopping.vue"],"names":[],"mappings":";;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;AAgIA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA","file":"Shopping.vue","sourceRoot":"src/views/examples","sourcesContent":["<template>\n  <div class=\"wrapper\">\n    <parallax\n      class=\"page-header header-filter header-small\"\n      parallax-active=\"true\"\n      :style=\"headerStyle\"\n    >\n      <div class=\"container\">\n        <div class=\"md-layout\">\n          <div\n            class=\"md-layout-item md-size-66 md-medium-size-66 ml-auto mr-auto text-center\"\n          >\n            <h2 class=\"title\">\n              Shopping Page\n            </h2>\n          </div>\n        </div>\n      </div>\n    </parallax>\n    <div class=\"main main-raised\">\n      <div class=\"section pb-0 pt-0\">\n        <div class=\"container\">\n          <md-card class=\"md-card-plain\">\n            <md-card-content>\n              <h3 class=\"card-title\">\n                Shopping Cart\n              </h3>\n              <md-table\n                v-model=\"shoppingCartTable\"\n                class=\"table-shopping\"\n              >\n                <md-table-row\n                  slot=\"md-table-row\"\n                  slot-scope=\"{ item }\"\n                >\n                  <md-table-cell md-label=\"\">\n                    <div class=\"img-container\">\n                      <img\n                        :src=\"item.image\"\n                        alt=\"products\"\n                      >\n                    </div>\n                  </md-table-cell>\n                  <md-table-cell\n                    md-label=\"Product\"\n                    class=\"td-name\"\n                  >\n                    <a href=\"javascript:void(0)\">{{ item.product }}</a>\n                    <br>\n                    <small>{{ item.category }}</small>\n                  </md-table-cell>\n                  <md-table-cell md-label=\"Color\">\n                    {{\n                      item.color\n                    }}\n                  </md-table-cell>\n                  <md-table-cell md-label=\"Size\">\n                    {{ item.size }}\n                  </md-table-cell>\n                  <md-table-cell\n                    md-label=\"Price\"\n                    class=\"td-number\"\n                  >\n                    <small>€</small>\n                    {{ item.price }}\n                  </md-table-cell>\n                  <md-table-cell\n                    md-label=\"Qty\"\n                    class=\"td-number\"\n                  >\n                    {{ item.qty }}\n                    <div class=\"md-group\">\n                      <md-button\n                        class=\"md-round md-info md-sm\"\n                        @click=\"increaseQuantity(item)\"\n                      >\n                        <md-icon>add</md-icon>\n                      </md-button>\n                      <md-button\n                        class=\"md-round md-info md-sm\"\n                        @click=\"decreaseQuantity(item)\"\n                      >\n                        <md-icon>remove</md-icon>\n                      </md-button>\n                    </div>\n                  </md-table-cell>\n                  <md-table-cell\n                    md-label=\"Amount\"\n                    class=\"td-number\"\n                  >\n                    <small>€</small>\n                    {{ item.amount }}\n                  </md-table-cell>\n                  <md-table-cell>\n                    <md-button\n                      class=\"md-just-icon md-round md-simple\"\n                    >\n                      <md-icon>close</md-icon>\n                    </md-button>\n                  </md-table-cell>\n                </md-table-row>\n              </md-table>\n              <div class=\"table table-stats\">\n                <div class=\"td-price\">\n                  <div class=\"td-total\">\n                    Total\n                  </div>\n                  <span>\n                    <small>€</small>\n                    {{ shoppingTotal }}\n                  </span>\n                </div>\n                <div class=\"text-right\">\n                  <md-button class=\"md-info md-round\">\n                    Complete Purchase\n                    <md-icon>keyboard_arrow_right</md-icon>\n                  </md-button>\n                </div>\n              </div>\n            </md-card-content>\n          </md-card>\n        </div>\n      </div>\n    </div>\n  </div>\n</template>\n\n<script>\nimport Mixins from \"@/plugins/basicMixins\";\n\nexport default {\n  mixins: [Mixins.ShoppingCart, Mixins.HeaderImage],\n  data() {\n    return {\n      image: require(\"@/assets/img/examples/bg2.jpg\"),\n      shoppingCartTable: [\n        {\n          image: require(\"@/assets/img/product1.jpg\"),\n          product: \"Spring Jacket\",\n          category: \"by Dolce&Gabbana\",\n          color: \"Red\",\n          size: \"M\",\n          price: 549,\n          qty: 1,\n          amount: 549\n        },\n        {\n          image: require(\"@/assets/img/product2.jpg\"),\n          product: \"Short Pants\",\n          category: \"by Gucci\",\n          color: \"Purple\",\n          size: \"M\",\n          price: 499,\n          qty: 2,\n          amount: 998\n        },\n        {\n          image: require(\"@/assets/img/product3.jpg\"),\n          product: \"Pencil Skirt\",\n          category: \"by Valentino\",\n          color: \"Red\",\n          size: \"M\",\n          price: 799,\n          qty: 1,\n          amount: 799\n        }\n      ]\n    };\n  }\n};\n</script>\n\n<style lang=\"scss\"></style>\n"]}]}